<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="oob.instagramapitest">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".ApplicationComponent.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning,InnerclassSeparator">
        <activity
            android:name=".HomeComponent.Framework.HomeActivity"
            android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".OptionsComponent.Framework.OptionsActivity"
            android:screenOrientation="portrait">

        </activity>

        <activity
            android:name=".AddPhotoScheduleComponent.Framework.AddPhotoScheduleActivity"
            android:screenOrientation="portrait">

        </activity>

        <activity
            android:name=".Z_Deprecated.GetImageActivity"
            android:screenOrientation="portrait"
            tools:ignore="InnerclassSeparator" />

        <receiver
            android:name=".Z_Deprecated.AlarmReceiver"
            android:exported="false"
            tools:ignore="InnerclassSeparator" />
    </application>
</manifest>